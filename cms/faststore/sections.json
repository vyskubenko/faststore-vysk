          "title": "navButtons Item List",
          "type": "array",
          "items": {
            "title": "navButtons Item",
            "type": "object",
            "properties": {
              "title": {
                "title": "Text",
                "type": "string"
              },
              "icon": {
                "title": "icon",
                "type": "string",
                "widget": { "ui:widget": "image-uploader" }
              },
              "href": {
                "title": "href",
                "type": "string"
              }
            }
          }
        },
        "menu": {
          "title": "Main Menu Item List",
          "type": "array",
          "items": {
            "title": "main menu first level",
            "type": "object",
            "properties": {
              "title": {
                "title": "Text",
                "type": "string"
              },
              "href": {
                "title": "href",
                "type": "string"
              },
              "submenu": {
                "title": "2nd Level Main Menu Item List",
                "type": "array",
                "items": {
                  "title": "Items in second level menu",
                  "type": "object",
                  "properties": {
                    "title": {
                      "title": "Text",
                      "type": "string"
                    },
                    "href": {
                      "title": "href",
                      "type": "string"
                    },
                    "headTitle": {
                      "title": "Is head title?",
                      "type": "boolean",
                      "default": false
                    },
                    "submenu": {
                      "title": "3nd Level Main Menu Item List",
                      "type": "array",
                      "items": {
                        "title": "Items in third level menu",
                        "type": "object",
                        "properties": {
                          "title": {
                            "title": "Text",
                            "type": "string"
                          },
                          "href": {
                            "title": "href",
                            "type": "string"
                          },
                          "image": {
                            "title": "image",
                            "type": "string",
                            "widget": { "ui:widget": "image-uploader" }
                          },
                          "headTitle": {
                            "title": "Is head title?",
                            "type": "boolean",
                            "default": false
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
